{
  "name": "testing-nodemodulus",
  "version": "1.0.0",
  "description": "Electron app with portable MongoDB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "electron-log": "^5.2.2",
    "mongoose": "^8.8.1",
    "mongodb-memory-server": "^8.12.2"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/mongodb-memory-server/**"
    ],
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "appId": "com.example.testingnodemodulus"
  }
}
